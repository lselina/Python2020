import random

num = random.randint(1, 100)

guess = int(input("Guess a number between 1 and 100: "))

while guess != num:
  if guess > num:
    print("Too high! Guess again.")
    guess = int(input("Guess a number between 1 and 100: "))

  if guess < num: 
    print("Too low! Guess again.")
    guess = int(input("Guess a number between 1 and 100: "))

  if guess == num:
    print("Correct! You guessed the number.")
